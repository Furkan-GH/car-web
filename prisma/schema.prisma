generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum CarStatus {
  NONE
  BARRIER
  WATERTANK
  FAN
  CAMERA
  FINISH
}

model WashedCar {
  id String @id @default(cuid())
  status CarStatus @default(NONE)
  data Data[]
  createdDate DateTime
}

model Data {
  id            String    @id @default(cuid())
  name          String
  sensorData  Json?
  washedCarId String
  washedCar WashedCar @relation(fields: [washedCarId],references: [id],onDelete:Cascade)
}
